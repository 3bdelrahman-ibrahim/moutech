{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    } 
  ],
  "routes": [
    {
      "src": "/product/(.*)",
      "dest": "/product.html"
    },
    {
      "src": "/checkout",
      "dest": "/checkout.html"
    },
    {
      "src": "/products",
      "dest": "/products.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
